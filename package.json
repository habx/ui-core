{
  "name": "@habx/ui-core",
  "version": "3.1.1",
  "description": "HABX Design System",
  "sideEffects": false,
  "main": "./dist/index.cjs.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/typings/index.d.ts",
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "npm run clean && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "bili",
    "build:watch": "npm run clean && tsc --watch",
    "lint": "eslint \"src/**\"",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "storybook:build": "build-storybook -c .storybook -o .storybook-static",
    "storybook:deploy": "storybook-to-ghpages -s storybook:build --ci -t=GITHUB_TOKEN",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "type:coverage": "tscov"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/habx/ui-core.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/habx/ui-core/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
  "homepage": "https://github.com/habx/ui-core#readme",
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "styled-components": "^5.0.0"
  },
  "dependencies": {
    "@delangle/use-modal": "^1.4.0",
    "color": "^3.1.2",
    "lodash.deburr": "^4.1.0",
    "markdown-it": "^10.0.0",
    "markdown-it-sup": "^1.0.0",
    "merge": "^1.2.1",
    "react-easy-swipe": "0.0.18",
    "react-is": "^16.12.0"
  },
  "devDependencies": {
    "@habx/eslint-config-client": "^3.0.1",
    "@liftr/tscov": "^1.4.1",
    "@storybook/addon-actions": "^5.3.12",
    "@storybook/addon-centered": "^5.3.12",
    "@storybook/addon-knobs": "^5.3.12",
    "@storybook/addon-viewport": "^5.3.12",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react": "^5.3.12",
    "@storybook/storybook-deployer": "^2.8.1",
    "@storybook/theming": "^5.3.13",
    "@testing-library/jest-dom": "^5.0.1",
    "@testing-library/react": "^10.0.2",
    "@types/color": "^3.0.0",
    "@types/jest": "^25.1.0",
    "@types/lodash.deburr": "^4.1.6",
    "@types/markdown-it": "0.0.9",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "@types/react-is": "^16.7.1",
    "@types/sinon": "^9.0.0",
    "@types/styled-components": "^5.0.0",
    "babel-loader": "^8.0.6",
    "bili": "^4.8.1",
    "fork-ts-checker-webpack-plugin": "^4.0.3",
    "husky": "^4.0.7",
    "jest": "^25.1.0",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "lint-staged": "^10.0.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "rimraf": "^3.0.0",
    "rollup-plugin-typescript2": "^0.27.0",
    "sinon": "^9.0.0",
    "storybook-addon-designs": "^5.2.0",
    "styled-components": "^5.0.1",
    "styled-normalize": "^8.0.6",
    "ts-jest": "^25.0.0",
    "ts-loader": "^7.0.0",
    "typescript": "^3.8.3"
  },
  "storybook-deployer": {
    "commitMessage": "[SKIP CI] Deploy Storybook to GitHub Pages"
  }
}
